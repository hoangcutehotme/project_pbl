{
    "version": 2,
    "builds": [
        {
            "src": "flask_api.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "flask_api.py"
        }
    ]
}